// cSpell Settings
{
    // Version of the setting file.  Always 0.2
    "version": "0.2",
    // language - current active spelling language
    "language": "en",
    "gitignoreRoot": ".",
    "useGitignore": true,
    "dictionaries": [
        "css",
        "html",
        "fonts",
        "typescript",
        "softwareTerms",
        "companies",
        "lorem-ipsum",
        "docker",
        "bash",
        "python"
    ],
    // words - list of words to be always considered correct
    "words": [
        "acemcorp",
        "acff",
        "Acmecorp",
        "addprefix",
        "APIM",
        "Asana",
        "azp",
        "Creativecommons",
        "cursoring",
        "datawire",
        "datetime",
        "Datetime",
        "decompile",
        "freemium",
        "gsutil",
        "hadolint",
        "ingressclasses",
        "ipallowlist",
        "Keycloak",
        "kubernetesingress",
        "linkcheck",
        "mydomain",
        "myworkspace",
        "openapi",
        "otel",
        "Proxying",
        "redirectscheme",
        "signup",
        "sublicensable",
        "sunsetting",
        "Telemtry",
        "traefik",
        "Traefik",
        "traefiklabs",
        "unversioned",
        "uskkk"
    ],
    // flagWords - list of words to be always considered incorrect
    // This is useful for offensive words and common spelling errors.
    // For example "hte" should be "the"
    "flagWords": [
        "hte"
    ],
    "ignorePaths": [
        "CHANGELOG.md",
        "package.json",
        "yarn.lock",
        ".lefthook.yml",
        "outline.txt",
        "archive"
    ]
}
